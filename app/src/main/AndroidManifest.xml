<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.yanccprogramador.blogti">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:logo="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".logar"
            android:icon="@mipmap/logo"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            android:logo="@mipmap/logo">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:logo="@mipmap/logo"
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme" />
        <activity android:name=".ActivityPublish" android:logo="@mipmap/logo" />
        <activity android:name=".br.com.yanccprogramador.blogti.multidex.MyApplication" android:logo="@mipmap/logo" />
        <activity android:name=".ActivityMine" android:logo="@mipmap/logo"></activity>
    </application>

</manifest>